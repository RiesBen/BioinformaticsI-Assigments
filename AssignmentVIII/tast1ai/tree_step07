// suffix tree
digraph {
	A [label=""]
	B [label=""]
	C [label=""]
	D [label=""]
	E [label=""]
	F [label=""]
	G [label=""]
	H [label=""]
	I [label=""]
	J [label=""]
	K [label=""]
		A -> B [label="CTAGTAGCAG$"]
		A -> C [label=TAG]
		A -> D [label=AG]
		A -> E [label=G]
		C -> F [label="TAGCAG$"]
		C -> G [label="CAG$"]
		D -> H [label="TAGCAG$"]
		D -> I [label="CAG$"]
		E -> J [label="TAGCAG$"]
		E -> K [label="CAG$"]
}